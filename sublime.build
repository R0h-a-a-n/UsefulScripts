    "shell_cmd": "/opt/homebrew/bin/g++-15 -std=c++17 -Ofast -march=native -flto -fno-math-errno -ffast-math -funroll-loops -fomit-frame-pointer -pipe \"${file}\" -o \"${file_base_name}\" && ./\"${file_base_name}\" < inputf.in > outputf.out",
